
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Customers</title>
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
            <th>Age</th>
        </tr>
        @foreach(ASP.Models.Customer customer in ViewBag.Customers as List<ASP.Models.Customer>)
        {
            <tr>
                <td>@customer.FirstName @customer.LastName</td>
                <td>@customer.Age</td>
            </tr>
        }
    </table>
</body>
</html>
